@model IEnumerable<FarmManager.Models.Vaca>

@{
    ViewBag.Title = "Index";
}

<h2>Lista Gado</h2>

<p>
    @Html.ActionLink("Cadastrar Novo Boi", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Brinco
        </th>
        <th>
            Data Nascimento
        </th>
        <th>
            Sexo
        </th>
        <th>
            Data Desamamentação
        </th>
        <th>
            Data Última Inseminação
        </th>
        <th>
            Data Última Procriação
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NRBrinco)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DTNascimento)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TPSexo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DTDesamamentacao)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DTInseminacao)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DTProcriacao)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.NRBrinco }) |
            @Html.ActionLink("Detalhes", "Details", new { id=item.NRBrinco }) |
            @Html.ActionLink("Deletar", "Delete", new { id=item.NRBrinco })
        </td>
    </tr>
}

</table>
