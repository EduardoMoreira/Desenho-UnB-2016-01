@model FarmManager.Models.Vaca

@{
    ViewBag.Title = "Details";
}

<h2>Detalhes</h2>

<div>
    <h4>Vaca @Html.DisplayFor(model => model.NRBrinco)</h4>
	<hr />
    <dl class="dl-horizontal">
        <dt>
            Data de nascimento
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DTNascimento)
        </dd>

        <dt>
            Sexo
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TPSexo)
        </dd>

        <dt>
            Data de inseminação
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DTInseminacao)
        </dd>

        <dt>
            Data de desamamentação
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DTDesamamentacao)
        </dd>

        <dt>
            Data de procriação
        </dt>

        <dd>
            @Html.DisplayFor(model => model.DTProcriacao)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Editar", "Edit", new { id = Model.NRBrinco }) |
    @Html.ActionLink("Voltar a lista de vacas", "Index")
</p>
